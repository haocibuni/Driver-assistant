page {
  background: #eee;
}
.row {
  overflow: hidden;
  line-height: 50px;
  border-bottom: 1px solid #ccc;
  color: #777;
  background: #fff;
}
.submit-button {
  margin:10px 8px 0 8px;
  background: #ff9700;
  line-height: 50px;
  border-radius: 6px;
  text-align: center;
  color: #fff;
}
.title{
  float:left;
  width:90px;
  padding-left:10px;
}
.info-input{
  height: 50px;
}
.success {
  background: #fff;
  padding-bottom: 5px;
  padding-top:30px;
}
.congratulation {
  text-align: center;
  font-size: 20px;
  line-height: 80px;
  
}
.success-icon {
  position: relative;
  top:4px;
  margin-right: 6px;
}
.backhome-button {
  margin: 20px;
}
.add{
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  background-color:  #03bbd5;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  float:left;
  justify-content: center;
}